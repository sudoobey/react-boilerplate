{
  "name": "react-boilerplate",
  "version": "0.0.0",
  "description": "React boilerplate by hgen",
  "scripts": {
    "start": "NODE_ENV=production node server/server",
    "test": "node_modules/.bin/mocha -R spec ./server/**/test-*.js",
    "clean": "rm -rf ./web-dist && rm -rf ./node-dist",
    "webpack": "node_modules/.bin/webpack",
    "bundle": "NODE_ENV=production npm run webpack",
    "bundle:dev": "NODE_ENV=development npm run webpack",
    "dev": "NODE_ENV=development node server/server",
    "static": "npm run bundle && node ./generate-static.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hgenru/react-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hgenru/react-boilerplate"
  },
  "homepage": "https://github.com/hgenru/react-boilerplate",
  "dependencies": {
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.1.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-mount": "^2.0.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-lite": "^0.15.14",
    "react-router": "^2.8.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "^0.1.1",
    "babel-plugin-react-require": "^2.1.0",
    "babel-plugin-system-import-transformer": "^2.3.0",
    "babel-plugin-transform-ensure-ignore": "^0.1.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "es-css-modules": "^1.2.3",
    "eslint": "^2.11.1",
    "eslint-config-google": "^0.5.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "fs-extra": "^0.30.0",
    "happypack": "^2.2.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.21.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "path-chunk-webpack-plugin": "^1.2.0",
    "postcss-cssnext": "^2.6.0",
    "postcss-loader": "^0.9.1",
    "postcss-modules": "^0.5.0",
    "react-hot-loader": "^1.3.0",
    "react-router-to-array": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.14.1"
  }
}
