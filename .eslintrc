{
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true,
  },
  extends: "google",
  "globals": {
    "console": false,
    "describe": false,
    "beforeEach": false,
    "it": false,
  },
  "rules": {
    "camelcase": 0,
    "indent": [2, 4, {"SwitchCase": 1}],
    "generator-star-spacing": 0,
    "require-jsdoc": 0,
    "arrow-parens": 0,
    "react/jsx-uses-vars": 1
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "React",
      "version": "15.0"
    }
  }
}
